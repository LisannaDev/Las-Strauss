{
    "builds": [
      {
        "src": "LS_frontend/lsfrontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "LS_frontend/lsfrontend/dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  